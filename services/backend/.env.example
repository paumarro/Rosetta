# OAuth2.0 Client ID
CLIENT_ID=your-client-id

# OAuth2.0 Client Secret
CLIENT_SECRET=your-client-secret

# Azure Tenant ID
TENANT_ID=your-tenant-id

# Rosetta Application Domain 
ROSETTA_DOMAIN=localhost:8080

# PostgreSQL Database Configuration
